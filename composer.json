{
    "name": "ibrahim/custom-auth",
    "description": "Custom Auth",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ibrahim\\CustomAuth\\": "scr/"
        }
    },
    "require": {
        "php": "^7.4|^8.0.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Ibrahim\\CustomAuth\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "IbrahimNeop2022",
            "email": "i.ismail@neopplanet.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibrahim\\CustomAuth\\Providers\\CustomAuthServiceProvider"
            ]
        }
    }
}
